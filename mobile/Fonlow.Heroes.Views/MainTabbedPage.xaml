<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui" Title="Tour of Heroes"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewsNS="clr-namespace:Fonlow.Heroes.Views"
             xmlns:vmNS="clr-namespace:Fonlow.Heroes.VM;assembly=Fonlow.Heroes.ViewModels"
             x:Class="Fonlow.Heroes.Views.MainTabbedPage"
            >
  <!--Pages can be added as references or inline-->
    <ContentPage Title="Dashboard" >
        <viewsNS:Dashboard x:Name="dashboard" BindingContext="{Binding}">
        </viewsNS:Dashboard>
    </ContentPage>
    <ContentPage Title="Heroes" >
        <viewsNS:HeroesView x:Name="heroesView" BindingContext="{Binding}">
        </viewsNS:HeroesView>
    </ContentPage>
</TabbedPage>